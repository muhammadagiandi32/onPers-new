{
  "expo": {
    "name": "onPers-new",
    "slug": "onPers-new",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon-onpers.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/icon-onpers.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "newArchEnabled": true,
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "This app requires access to the camera to take photos.",
        "NSPhotoLibraryUsageDescription": "This app requires access to the photo library to select images."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon-onpers.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ],
      "package": "com.muhagi32.onPersnew"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "aec3c64e-0b5b-4c2d-a982-a988f6802ce9"
      }
    }
  }
}
